{
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": ""
  },

  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "" }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },

  "Keycloak": {
    "AuthorizationUrl": ""
  },

  "Authentication": {
    "MetadataAddress": "",
    "ValidIssuer": "",
    "Audience": ""
  },
  
  "Redis": {
    "ConnectionString": "redis_container:6379",
    "DefaultCacheDuration": 60
  }
}
